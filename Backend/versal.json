{
    "installCommand":"npm install --legacy-peer-deps",
    "version":2,
    "build":[
        {
            "src":"api/index.js",
            "use": "@vercel/node"
        },
        {
            "src":"src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"api/index.js"
        }
    ]
}